{
  "name": "schema-org-adapter",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "buildBrowser": "browserify src/browser/SDOAdapterBrowser.js -s sdoAdapter | derequire -cm --toplevel > dist/browser/sdoAdapter.js",
    "buildBrowserMinified": "browserify src/browser/SDOAdapterBrowser.js -s sdoAdapter -g [uglifyify] | derequire -cm --toplevel > dist/browser/sdoAdapter.js",
    "buildNode": "browserify src/node/SDOAdapterNode.js -s sdoAdapter --no-builtins --no-bundle-external --no-bf | derequire -cm --toplevel > dist/node/sdoAdapter.js",
    "buildNodeMinified": "browserify src/node/SDOAdapterNode.js -s sdoAdapter --no-builtins --no-bundle-external --no-bf -g [uglifyify] | derequire -cm --toplevel > dist/node/sdoAdapter.js",
    "test": "jest",
    "genDoc": "jsdoc src/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^8.0.0",
    "browserify": "^16.5.0",
    "derequire": "^2.0.6",
    "jest": "^24.9.0",
    "jsonld": "^1.6.2",
    "superagent": "^5.1.0",
    "uglifyify": "^5.0.1"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  }
}
